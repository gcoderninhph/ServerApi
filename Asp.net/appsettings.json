{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ServerApi": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ServerApi": {
    "Security": {
      "EnableAuthentication": false,
      "RequireAuthenticatedUser": false
    },
    "WebSocket": {
      "Patterns": [ "/ws", "/socket" ],
      "KeepAliveIntervalMinutes": 2,
      "BufferSize": 8192
    },
    "TcpStream": {
      "Port": 5003,
      "BufferSize": 8192
    }
  }
}
